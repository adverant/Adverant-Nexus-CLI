{
  "name": "@adverant/nexus-cli-types",
  "version": "3.0.0",
  "description": "Shared TypeScript types and interfaces for Nexus CLI ecosystem",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run clean && npm run build",
    "publish:dry": "npm publish --dry-run"
  },
  "keywords": [
    "nexus",
    "types",
    "typescript",
    "cli",
    "sdk"
  ],
  "author": "Adverant AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adverant/nexus-cli.git",
    "directory": "packages/types"
  },
  "homepage": "https://github.com/adverant/nexus-cli#readme",
  "bugs": {
    "url": "https://github.com/adverant/nexus-cli/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
